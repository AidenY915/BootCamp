package Test;

public class Person{
	private String name;
	private int age;
	public Person(String name, int age) {
		this.name = name;
		this.age = age;
	}
	
	public void show() {
		System.out.println(name + " " + age);		
	}
}

public class PersonMain{
	
	public static void main(String[] args) {
		
	}
}